{
  "name": "typedoc-plugin-external-package-links",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@10.20.0",
  "description": "A TypeDoc plugin to add link mappings to external packages",
  "author": "ocavue <ocavue@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/typedoc-plugin-external-package-links#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ocavue/typedoc-plugin-external-package-links.git"
  },
  "bugs": "https://github.com/ocavue/typedoc-plugin-external-package-links/issues",
  "keywords": [
    "typedoc"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "example": "pnpm build && pnpm --filter example build",
    "fix": "eslint --fix . && prettier --write .",
    "test": "vitest",
    "typecheck": "tsc -b"
  },
  "devDependencies": {
    "@ocavue/eslint-config": "^3.6.1",
    "@ocavue/tsconfig": "^0.5.0",
    "@types/node": "^22.18.13",
    "eslint": "^9.38.0",
    "execa": "^9.0.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.5"
  }
}
